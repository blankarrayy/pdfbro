{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "lib": [
            "es2019",
            "dom"
        ],
        "resolveJsonModule": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "rootDir": "./"
    },
    "include": [
        "./**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}